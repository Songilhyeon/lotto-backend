generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model LottoNumber {
  drwNo          Int      @id // 회차 번호 (PK)
  drwNoDate      DateTime // 추첨일
  drwtNo1        Int
  drwtNo2        Int
  drwtNo3        Int
  drwtNo4        Int
  drwtNo5        Int
  drwtNo6        Int
  bnusNo         Int
  firstPrzwnerCo String
  firstWinamnt   String
  totSellamnt    String
  firstAccumamnt String
  createdAt      DateTime @default(now())
}
